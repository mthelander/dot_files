[core]
  editor = vim
[alias]
  co = checkout
  br = branch
  st = status
  lg = "log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative"
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = true
[rerere]
	enabled = true
